class ICS4UBrawl{
  public static void main(String[] args){
    Fighter testSubject = new Fighter("Aden Choi" , "Strength");
    Move swear  = new Move("Swear", 100.0, false, 5);
    testSubject.addMove(swear);
    System.out.println(testSubject.get_name() + " uses " + testSubject.get_moveSet().get(0).get_name());
    System.out.println("It dealt " + swear.get_basePower());
  }
}